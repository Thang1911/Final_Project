    <script>
        function exportToPdf(tableId) {
            const element = document.getElementById(tableId);

            const options = {
                margin: 1,
                filename: 'Report.pdf',
                jsPDF: { unit: 'in', format: 'letter', orientation: 'landscape' }
            };

            html2pdf().from(element).set(options).save();
        }
